                        <Button Margin="0,120,0,0" Height="28" Width="150" Content="Change Parameters" HorizontalAlignment="Right" x:Name="btnPlayerArgs" VerticalAlignment="Top" FontSize="14" Click="btnPlayerArgs_Click" />
                    </Grid>
                    <Label Height="34" HorizontalAlignment="Left" Margin="20,6,0,0" x:Name="lblPlayerStep1" VerticalAlignment="Top" Width="34" Foreground="#FFA8A8A8" Opacity="1" Content="❶" FontSize="14" />
