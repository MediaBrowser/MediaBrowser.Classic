<Window x:Class="Configurator.RenameForm"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Rename" Height="175" Width="450" Icon="/Configurator;component/Images/rename.ico" WindowStartupLocation="CenterOwner" ResizeMode="NoResize">
    <Grid>
        <Button Height="32" HorizontalAlignment="Right" Margin="0,0,20,20" Name="btnOK" VerticalAlignment="Bottom" Width="80" Click="btnOK_Click" FontSize="14">OK</Button>
        <Button Height="32" HorizontalAlignment="Right" Margin="0,0,110,20" Name="btnCancel" VerticalAlignment="Bottom" Width="80" Click="btnCancel_Click" FontSize="14">Cancel</Button>
        <TextBox Margin="20,50,20,0" Name="tbxName" Height="28" VerticalAlignment="Top" FontSize="14" />
        <Label Height="28" Margin="20,20,20,0" Name="lblName" VerticalAlignment="Top" FontSize="14">Label</Label>
    </Grid>
</Window>
